  color: #fff;
  border-radius: 5px;
  box-shadow: 0px 0px 5px #fff;
  border: none;
  margin-bottom: 10px;
  font-weight: bold;
  min-width: 100px; }

.menu-btn:active {
  background-color: #59B717;
  transition: 0.5s; }

@media (min-width: 1001px) {
