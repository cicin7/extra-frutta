@import url("https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap");
.header-section {
  background: #03262c;
  background: linear-gradient(180deg, #03262c 0%, #0d3036 100%); }

.header-container {
  max-width: 1200px;
  margin: 0 auto;
  padding: 15px 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center; }

.header-nav {
  display: flex;
  flex-direction: row;
  align-items: center;
  gap: 20px;
  margin: 10px 0;
  font-weight: 600;
  text-transform: uppercase;
  color: #fff;
  flex-wrap: wrap; }

.header-nav li {
  list-style-type: none; }

.header-nav ul li a {
  padding: 10px 20px;
  display: block; }

.header-nav a {
  color: #fff; }

.header-nav a:hover {
  transition: 0.8s;
  color: #59B717; }

.menu-btn {
  display: none;
  padding: 15px;
